    ></video>
    <div class="leadhead"> 
      <h1 style="margin-top: 7vh" class="leadhead">Leaderboard</h1>
    </div>
   
    <div class="table-container">
      <table>
        <thead>
          <tr>
            <th>Rank</th>
            <th>Team Name</th>
            <th>Score</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < teamsData.length; i++) { %>
          <tr>
            <td class="rank"><%= i + 1 %></td>
            <td class="team-name"><%= teamsData[i].teamName %></td>
            <td class="score"><%= teamsData[i].questionData.score %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
